    "tool",
    "toolbox"
  ],
  "author": "kui.liang.sg@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@vitest/coverage-v8": "^0.34.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1"
  },
  "dependencies": {
    "unwrapit": "^2.2.0"
  }
}
